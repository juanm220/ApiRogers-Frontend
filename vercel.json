{
  "rewrites": [
    {
      "source": "/api/cron",
      "destination": "/api/cron"
    },
    {
      "source": "/api/:path*",
      "destination": "https://apirogers-backend.onrender.com/api/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/10 * * * *"
    }
  ]
}
